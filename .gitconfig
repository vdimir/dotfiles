[alias]
    co = checkout
    st = status
    ci = commit -v
    lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %C(cyan)%G?%C(dim white) %an%C(reset)%C(bold yellow)%d%C(reset)'
    br = branch
    au = add -u
    sup = submodule update --init --recursive
    pushu=!"git push --set-upstream ${1:-origin} HEAD:vdimir/$(git symbolic-ref HEAD | sed -e 's/^refs\\/heads\\///')"
[puh]
    default = current
[pull]
    rebase = true
    ff = only
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
[fetch]
    prune = true
[core]
    autocrlf = input
[user]
    email = vdimir@clickhouse.com
    name = vdimir
    signingkey = B762CDA71370F2A61ED91E976EE4CE2BEDC51862
[gpg]
    program = gpg
[commit]
    gpgsign = true
[merge]
    ff = false
[rebase]
    autoStash = true
[init]
    defaultBranch = main
