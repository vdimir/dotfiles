{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.links": false,
    "editor.tokenColorCustomizations": {
        "[Base16 Grayscale Light]": {
            "comments": {
                "fontStyle": "",
                "foreground": "#606070"
            },
            "functions": "#404060",
            "variables": "#404060",
            "keywords": "#404040",
            "types": "#303030",
            "strings": "#303050",
            "textMateRules": [
                {
                    "scope": "entity.name.import.go",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "entity.name.type.class",
                    "settings": { "foreground": "#202020" }
                },
                {
                    "scope": "entity.name.function",
                    "settings": { "foreground": "#202020" }
                },
                {
                    "scope": "entity.name",
                    "settings": { "foreground": "#202020" }
                }
            ]
        },
        "[Base16 Grayscale Dark]": {
            "comments": {
                "fontStyle": "",
                "foreground": "#a0a0a0"
            },
            "functions": "#d4d4d4",
            "variables": "#a0a0a0",
            "keywords": "#a0a0a0",
            "types": "#a0a0a0",
            "strings": "#a0a0a0",
            "textMateRules": [
                {
                    "scope": "entity.name.import.go",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "entity.name.type.class",
                    "settings": { "foreground": "#d4d4d4" }
                },
                {
                    "scope": "entity.name.function",
                    "settings": { "foreground": "#d4d4d4" }
                },
                {
                    "scope": "entity.name",
                    "settings": { "foreground": "#d4d4d4" }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Base16 Grayscale Light]": {
            "editor.background": "#f8f8fa",
            "editorCursor.foreground": "#404040",
            "editor.foreground": "#404040",
            "editor.wordHighlightBorder": "#303030",
            // "editor.selectionHighlightBackground": "#f0f0f0",
            "editor.selectionHighlightBorder": "#ababab55",
            "editor.findMatchHighlightBackground": "#FFEEAAAA",
            "editor.findMatchBackground": "#FFDDAA",
            "diffEditor.removedTextBackground": "#f1e6e6",
            "diffEditor.insertedTextBackground": "#c4d1c323"
        },
        "[Base16 Grayscale Dark]": {
            "editor.background": "#1e1e1e",
            "editorCursor.foreground": "#d4d4d4",
            "editor.foreground": "#d4d4d4",
        },
    },
    "editor.rulers": [140],
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "files.hotExit": "off",
    "files.autoSave": "onFocusChange",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.occurrencesHighlight": false,
    "editor.renderWhitespace": "selection",
    "git.ignoreLegacyWarning": true,
    "window.restoreWindows": "none",
    "editor.codeLens": false,
    "git.autorefresh": true,
    "git.detectSubmodules": false,
    "editor.minimap.enabled": false,
    "rust-client.disableRustup": true,
    "search.searchOnType": false,
    "go.useLanguageServer": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "nasc-touchbar.addCursorBelow": false,
    "nasc-touchbar.goToDefinition": false,
    "nasc-touchbar.rename": false,
    "nasc-touchbar.showCommands": false,
    "cmake.configureOnOpen": true,
    "python.showStartPage": false,
    "github.gitAuthentication": false,
    "githubPullRequests.telemetry.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.fontSize": 13,
    "files.insertFinalNewline": true,
    "githubPullRequests.fileListLayout": "tree",
    "remoteHub.search.download.enabled": true,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "python.defaultInterpreterPath": "/usr/bin/python",
    "[plaintext]": {
        "editor.trimAutoWhitespace": false,
        "files.trimTrailingWhitespace": false
    },
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.blame.avatars": false,
    "gitlens.blame.format": "[${author|7}] ${message|5} ${agoOrDate|10-}",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "files.watcherExclude": {
        "contrib/": true
    },
    "search.exclude": {
        "contrib/": true
    },
    "go.toolsManagement.autoUpdate": true,
    "clock.dateFormat": "ddd, dS mmm | HH:MM",
    "gitlens.currentLine.enabled": false,
    "gitlens.statusBar.command": "gitlens.toggleFileBlame",
    "sortLines.filterBlankLines": true,
    "workbench.colorTheme": "Default Light+",
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "clangd.path": "/opt/homebrew/opt/llvm/bin/clangd",
    "cmake.additionalKits": [
        "/opt/homebrew/opt/llvm/bin/",
        "/opt/homebrew/opt/llvm/bin/clang"
    ],
    "editor.unicodeHighlight.allowedLocales": {
        "ru": true
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "security.workspace.trust.banner": "always",
    "security.workspace.trust.enabled": false
}
