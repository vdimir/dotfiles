{
    "telemetry.telemetryLevel": "crash",
    "editor.links": false,
    "editor.tokenColorCustomizations": {
        "[Base16 Grayscale Light]": {
            "comments": {
                "fontStyle": "",
                "foreground": "#606070"
            },
            "functions": "#404060",
            "variables": "#404060",
            "keywords": "#404040",
            "types": "#303030",
            "strings": "#303050",
            "textMateRules": [
                {
                    "scope": "entity.name.import.go",
                    "settings": { "fontStyle": "" }
                },
                {
                    "scope": "markup.list",
                    "settings": { "foreground": "#404040" }
                },
                {
                    "scope": ["markup.italic", "markup.bold"],
                    "settings": { "foreground": "#555555" }
                },
                {
                    "scope": ["entity.name.type.class", "entity.name.function", "entity.name"],
                    "settings": { "foreground": "#202020" }
                },
            ]
        },
        "[Base16 Grayscale Dark]": {
            "comments": {
                "fontStyle": "",
                "foreground": "#a0a0a0"
            },
            "functions": "#d4d4d4",
            "variables": "#a0a0a0",
            "keywords": "#a0a0a0",
            "types": "#a0a0a0",
            "strings": "#a0a0a0",
            "textMateRules": [
                {
                    "scope": "entity.name.import.go",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "entity.name.type.class",
                    "settings": { "foreground": "#d4d4d4" }
                },
                {
                    "scope": "entity.name.function",
                    "settings": { "foreground": "#d4d4d4" }
                },
                {
                    "scope": "entity.name",
                    "settings": { "foreground": "#d4d4d4" }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Base16 Grayscale Light]": {
            "editor.background": "#f8f8fa",
            "editorCursor.foreground": "#404040",
            "editor.foreground": "#404040",
            "editor.wordHighlightBorder": "#303030",
            "editor.selectionHighlightBackground": "#f0f0f0",
            "editor.selectionHighlightBorder": "#ababab55",
            "editor.findMatchHighlightBackground": "#FFEEAAAA",
            "editor.findMatchBackground": "#FFDDAA",
            "diffEditor.removedTextBackground": "#f1e6e6",
            "diffEditor.insertedTextBackground": "#c4d1c323"
        },
        "[Base16 Grayscale Dark]": {
            "editor.background": "#1e1e1e",
            "editorCursor.foreground": "#d4d4d4",
            "editor.foreground": "#d4d4d4",
        },
    },
    "editor.rulers": [140],
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "files.hotExit": "off",
    "files.autoSave": "onFocusChange",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.occurrencesHighlight": false,
    "editor.renderWhitespace": "selection",
    "git.ignoreLegacyWarning": true,
    "window.restoreWindows": "none",
    "git.autorefresh": true,
    "git.detectSubmodules": false,
    "search.searchOnType": false,
    "go.useLanguageServer": true,
    "cmake.configureOnOpen": true,
    "githubPullRequests.telemetry.enabled": false,
    "editor.minimap.showSlider": "always",
    "files.insertFinalNewline": true,
    "githubPullRequests.fileListLayout": "tree",
    "remoteHub.search.download.enabled": true,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "[plaintext]": {
        "editor.trimAutoWhitespace": false,
        "files.trimTrailingWhitespace": false
    },
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.blame.avatars": false,
    "gitlens.blame.format": "[${author|7}] ${message|5} ${agoOrDate|10-}",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "files.watcherExclude": {
        "contrib/": false,
        "build/": true,
        "build_/": true,
    },
    "search.exclude": {
        "contrib/": false,
        "build/": true,
        "build_/": true,
    },
    "files.exclude": {
        "contrib/": false,
        "build/": true,
        "build_/": true,
    },
    "go.toolsManagement.autoUpdate": true,
    "clock.dateFormat": " H:MM Z",
    "gitlens.currentLine.enabled": false,
    "gitlens.statusBar.command": "gitlens.toggleFileBlame",
    "sortLines.filterBlankLines": true,
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "clangd.path": "/opt/homebrew/opt/llvm/bin/clangd",
    "cmake.additionalKits": [
        "/opt/homebrew/opt/llvm/bin/",
        "/opt/homebrew/opt/llvm/bin/clang"
    ],
    "editor.unicodeHighlight.allowedLocales": {
        "ru": true
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "security.workspace.trust.banner": "always",
    "security.workspace.trust.enabled": false,
    "openInGitHub.remote.name": "upstream",
    "openInGitHub.useLocalBranch": false,
    "editor.inlineSuggest.enabled": true,
    "editor.cursorBlinking": "smooth",
    "editor.inlayHints.enabled": "off",
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true,
        "cpp": true
    },
    "editor.minimap.enabled": false,
    "terminal.integrated.showExitAlert": false,
    "clangd.arguments": [
        "--header-insertion=never"
    ],
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.tabs.enableAnimation": false,
    "terminal.integrated.wordSeparators": " ()[]{}',\"`─‘’~/.-_$!:",
    "clock.iconName": "",
    "clock.alignment": "Right",
    "python.formatting.provider": "black",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "terminal.integrated.cursorWidth": 2,
    "githubPullRequests.pullBranch": "never",
    "diffEditor.ignoreTrimWhitespace": false,
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user}"
        }
    ],
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Base16 Grayscale Light",
    "workbench.preferredDarkColorTheme": "Base16 Grayscale Light",
    "workbench.colorTheme": "Base16 Grayscale Light",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "gitlens.plusFeatures.enabled": false,
    "gitlens.virtualRepositories.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.format": "${agoOrDate}${' via 'pullRequest}",
    "github.gitProtocol": "ssh",
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "aws-dev": "linux"
    },
    "window.commandCenter": false,
    "terminal.integrated.tabs.enabled": false,
    "editor.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "window.zoomLevel": 1,
}
